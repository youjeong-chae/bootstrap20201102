<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
<title>Insert title here</title>
</head>
<body>
<div class="container"> 
<!-- container는 화면을 꽉 채우지 않음. 보통 container씀 좀더 예뻐서 -->
<!-- 공백은 화면 크기에 따라 달라짐 -->
<!-- table.table>tr>th*3>lorem1^^tr*3>td*3>lorem1 -->
<table class="table">
	<tr>
		<th>Lorem.</th>
		<th>Vero!</th>
		<th>Consequatur.</th>
	</tr>
	<tr>
		<td>Lorem.</td>
		<td>Ea!</td>
		<td>Officia.</td>
	</tr>
	<tr>
		<td>Lorem.</td>
		<td>Pariatur?</td>
		<td>Voluptas?</td>
	</tr>
	<tr>
		<td>Lorem.</td>
		<td>Voluptatum.</td>
		<td>Porro!</td>
	</tr>
</table>

</div>

<div class="container-fluid">	
<!-- container-fluid는 화면을 꽉 채움 -->
<table class="table">
	<tr>
		<th>Lorem.</th>
		<th>Unde.</th>
		<th>Odit!</th>
	</tr>
	<tr>
		<td>Lorem.</td>
		<td>Consequuntur.</td>
		<td>Facere?</td>
	</tr>
	<tr>
		<td>Lorem.</td>
		<td>Iste.</td>
		<td>Tempore?</td>
	</tr>
	<tr>
		<td>Lorem.</td>
		<td>Expedita.</td>
		<td>Repudiandae!</td>
	</tr>
</table>
</div>
</body>
</html>